
<%= search_form_for @q, html: {class: "pull-right form-search article_search"} do |f| %>
    <%= f.text_field :start_time_date_equals, placeholder: "Date" %>
    <%= f.submit "Search", class: "btn" %>
<% end %>

Results
<% @q.limit(10).each do |f| %>
    <%= (f.doctor.name)%>
    <%= (f.patient.name)%>
    <%=  f.start_time %>
    <%=  f.end_time %><br/>
<% end %>
