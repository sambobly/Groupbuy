
<div class="-1u 10u options">
	<div class="title">Options</div>

</div>
<%= form_tag( url_for(controller: 'appointments', :action => 'find' ), :method => 'get' ) do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :first_name => nil  %>
    </p>
 <% end %>

<% @appointments.each do |appointment| %>
    <p><%= appointment.id %></p>
    <% end %>
<div id="calendar" class="-1u 10u">
  <%= form_tag( url_for(controller: 'appointments', :action => 'findNextAvailableSlot' ), :method => 'get' ) do %>      <p>
        <%= text_field_tag :find, params[:search] %>
        <%= submit_tag "Find", :first_name => nil  %>
      </p>
  <% end %>
  <% @appointments.each do |appointment| %>
      <p><%= appointment.name %></p>
  <% end %>
  <%= search_form_for @search do |f| %>
      <div class="field">
        <%= f.label :start_time_cont, "Date" %>
        <%= f.text_field :start_time_cont %>
      </div>
      <div class="actions"><%= f.submit "Search" %></div>

  <% end %>

