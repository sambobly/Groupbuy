<h1>Listing consult_templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consult_templates.each do |consult_template| %>
      <tr>
        <td><%= consult_template.name %></td>
        <td><%= consult_template.content %></td>
        <td><%= link_to 'Show', consult_template %></td>
        <td><%= link_to 'Edit', edit_consult_template_path(consult_template) %></td>
        <td><%= link_to 'Destroy', consult_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consult template', new_consult_template_path %>
